<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Arf UI Test Suite" parallel="tests" data-provider-thread-count="1">
    <listeners>
        <listener class-name="listener.ExecutionListener"/>
        <listener class-name="listener.TestListener"/>
        <listener class-name="listener.AnnotationTransformer"/>
    </listeners>
    <test name="Run All Tests">
        <classes>
            <class name="io.cucumber.skeleton.RunCucumberTest"/>
        </classes>
    </test>
</suite>